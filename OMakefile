  sed s/beanish.svg/beanish0012.svg/ languages/example-docinfo.html > /tmp/languages-docinfo.html

gh-pages/languages.html: gh-pages/beanish0012.svg /tmp/languages.adoc /tmp/languages-docinfo.html
  asciidoctor /tmp/languages.adoc -o gh-pages/languages.html