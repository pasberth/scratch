gh-pages/widkim.svg: languages/widkim.jade
  jade < languages/widkim.jade > gh-pages/widkim.svg

gh-pages/widkim.eot: gh-pages/widkim.svg
  curl -X POST 'https://ofc.p.mashape.com/directConvert/' -H "X-Mashape-Authorization: $(MASHAPE_KEY)" -F file=@gh-pages/widkim.svg -F format=eot > /tmp/widkim.eot.tar.gz
  tar -xf /tmp/widkim.eot.tar.gz -C /tmp
  cp /tmp/onlinefontconverter.com/converted-files/widkim.eot gh-pages/widkim.eot

gh-pages/widkim.ttf: gh-pages/widkim.svg
  curl -X POST 'https://ofc.p.mashape.com/directConvert/' -H "X-Mashape-Authorization: $(MASHAPE_KEY)" -F file=@gh-pages/widkim.svg -F format=ttf > /tmp/widkim.ttf.tar.gz
  tar -xf /tmp/widkim.ttf.tar.gz -C /tmp
  cp /tmp/onlinefontconverter.com/converted-files/widkim.ttf gh-pages/widkim.ttf

gh-pages/widkim.woff: gh-pages/widkim.svg
  curl -X POST 'https://ofc.p.mashape.com/directConvert/' -H "X-Mashape-Authorization: $(MASHAPE_KEY)" -F file=@gh-pages/widkim.svg -F format=woff > /tmp/widkim.woff.tar.gz
  tar -xf /tmp/widkim.woff.tar.gz -C /tmp
  cp /tmp/onlinefontconverter.com/converted-files/widkim.woff gh-pages/widkim.woff

gh-pages/languages.html: languages/example.adoc languages/example-docinfo.html
  asciidoctor languages/example.adoc -o gh-pages/languages.html

gh-pages/rounsic.html: completion/rounsic.adoc completion/rounsic-docinfo.html
  asciidoctor completion/rounsic.adoc -o gh-pages/rounsic.html